<div class="new-details d-flex jc-center">
    <div class="container mt-16 ">

        <div class="current-new d-flex">
            <div class="content col-66 item-content" data-news="{{data.ma_so}}" data-da-like="{{data.da_like}}">

                <div class="images-news">
                    <div class="image-current">
                        <img src="/{{images.[0]}}" alt="" id="main-image">
                    </div>
                    <div class="image-review ">
                        <div class=" d-flex jc-arrow owl-carousel">
                            {{#each images}}
                            <div class="item">
                                <img src="/{{this}}" alt="" class="carousel-image">
                            </div>
                            {{/each}}
                        </div>
                    </div>
                </div>
                <div class="heading">
                    <span class="span-text  mt-16">Cho thuê/ {{diaLyData.ten_thanh_pho}}/ {{diaLyData.ten_quan_huyen}}/
                        <span class="dark-text">Nhà trọ, phòng trọ tại {{sonha}}</span>
                    </span>
                    <h2 class="title-text mt-12">{{data.tieu_de}}</h2>
                    <span class="span-text  mt-12">Cho thuê/ {{diaLyData.ten_thanh_pho}}/ {{diaLyData.ten_quan_huyen}}/
                        <span class="dark-text">Nhà trọ, phòng trọ tại {{sonha}}</span></span>
                </div>

                <div class="info-base d-flex jc-between mt-16">
                    <div class="description d-flex">
                        <div class="item">
                            <span class="span-text">Mức giá</span>
                            <span class="desc-text">{{formattedPrice}} triệu/tháng

                            </span>
                        </div>
                        <div class="item">
                            <span class="span-text">Diện tích</span>
                            <span class="desc-text">{{data.dien_tich}} m²
                            </span>
                        </div>
                        <div class="item">
                            <span class="span-text">Phòng ngủ</span>
                            <span class="desc-text">{{noiDungThem.bedroom}} Phòng ngủ
                            </span>
                        </div>
                    </div>

                    <div class="action d-flex ai-center">
                        <div class="btn btn-share">
                            <i class="fa-solid fa-share-nodes"></i>
                        </div>
                        <div class="btn btn-warning">
                            <i class="fa-solid fa-triangle-exclamation"></i>
                        </div>
                        <div class="btn btn-like">
                            {{#if data.da_like}}
                            <i class="fa-solid fa-heart"></i>
                            {{else}}
                            <i class="fa-regular fa-heart"></i>
                            {{/if}}
                        </div>
                    </div>
                </div>
                <div class="desc-details">
                    <h3 class="heading-text">Thông tin mô tả</h3>
                    <div class="content mt-12">

                        {{{noi_dung}}}

                    </div>
                    <div class="contact mt-12">

                        {{!-- <i class="fa-solid fa-phone-volume"></i> --}}
                        <span>Liên hệ: </span>
                        <span>{{data.so_dien_thoai}}</span>
                        <span class="">·</span>
                        <button class="btn-phone btn pd--8-12 mr-8"> Hiện số</button>

                    </div>
                </div>
                <div class="features  ">
                    <h3 class="heading-text">Đặc điểm phòng trọ</h3>
                    <div class="d-flex">

                        <div class="item col-50  d-flex ai-center">
                            <span class="span-text">
                                <i class="fa-solid fa-expand"></i>
                            </span>
                            <span class="span-text">Diện tích</span>
                            <span class="span-text">{{data.dien_tich}} m²</span>
                        </div>
                        <div class="item col-50  d-flex ai-center">
                            <span class="span-text">
                                <i class="fa-solid fa-money-check-dollar"></i> </span>
                            <span class="span-text">Mức giá</span>
                            <span class="span-text">{{formattedPrice}} triệu/tháng</span>
                        </div>
                        <div class="item col-50  d-flex ai-center">
                            <span class="span-text">
                                <i class="fa-solid fa-city"></i> </span>
                            <span class="span-text">Số tầng</span>
                            <span class="span-text">{{noiDungThem.floor}} tầng</span>
                        </div>
                        <div class="item col-50  d-flex ai-center">
                            <span class="span-text">
                                <i class="fa-solid fa-bed"></i> </span>
                            <span class="span-text">Số phòng ngủ</span>
                            <span class="span-text">{{noiDungThem.bedroom}} phòng</span>
                        </div>
                        <div class="item col-50  d-flex ai-center">
                            <span class="span-text">
                                <i class="fa-solid fa-bath"></i>
                            </span>
                            <span class="span-text">Số toilet</span>
                            <span class="span-text">{{noiDungThem.toilet}} phòng</span>
                        </div>
                        <div class="item col-50  d-flex ai-center">
                            <span class="span-text">
                                <i class="fa-solid fa-expand"></i>
                            </span>
                            <span class="span-text">Nội thất</span>
                            <span class="span-text">{{noiDungThem.interior}}</span>
                        </div>
                    </div>
                </div>
                <div class="feedback  ">
                    <h3 class="heading-text">Đánh giá bài viết</h3>

                    <div class="d-flex jc-between pd--14-16 user-content ">
                        {{#each feedbackResult}}
                        <div class="d-flex feedback-user col-100">
                            <img src="/{{this.anh_dai_dien}}" alt="" width="40" height="40" class="avatar mr-8 ">
                            <div>
                                <span class="span-text">{{this.ten_nguoi_dung}}</span>
                                <span class="span-text star">
                                    {{renderStars this.diem_danh_gia}}</span>
                                <span class="span-text">Ngày đánh giá - {{this.ngay_tao}}</span>
                                <span class="span-text comment">{{this.binh_luan}}</span>
                            </div>
                        </div>
                        {{/each}}
                    </div>
                </div>
            </div>
            <div class="user-post col-33">
                <div class="d-flex jc-between  user-content  ">
                    <div class="d-flex info-user ai-center col-100">
                        <img src="/{{data.anh_dai_dien}}" alt="" width="40" height="40" class="avatar mr-8 ">
                        <div>
                            <span class="span-text name">{{data.ten_nguoi_dung}}</span>
                            <span class="span-text">Đã truy cập hôm qua</span>
                        </div>
                    </div>
                    <div class="col-100 contact-user">
                        <h3 class="heading-text">Liên hệ người bán</h3>
                        <div class="mt-12 jc-arrow">
                            <a class="btn-phone btn  mr-8 d-flex ai-center jc-between"
                               href="tel:{{data.so_dien_thoai}}">
                                <div class="d-flex">

                                    <span class="span-text">

                                        <i class="fa-solid fa-phone-volume"></i>
                                    </span>
                                    <span class="span-text">{{data.so_dien_thoai}}</span>
                                </div>
                                {{!-- <span class="">·</span> --}}
                                <span class="span-text">Hiện số</span>
                            </a>

                            <div class="btn-mail btn  mr-8 d-flex ai-center jc-between">
                                <div class="d-flex">

                                    <span class="span-text">

                                        <i class="fa-regular fa-message"></i>
                                    </span>
                                    {{!-- <span class="span-text">0923645837</span> --}}
                                </div>
                                {{!-- <span class="">·</span> --}}
                                <span class="span-text">Chat với người bán</span>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>

